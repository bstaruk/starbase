{
  "name": "starbase",
  "version": "3.5.0",
  "author": "Brian Staruk <brian@staruk.net>",
  "contributors": [
    {
      "name": "Brian Staruk",
      "email": "brian@staruk.net",
      "url": "https://brian.staruk.net"
    }
  ],
  "description": "Production-ready website boilerplate made with webpack 5, modern JS (via Babel) & Sass",
  "homepage": "https://github.com/bstaruk/starbase",
  "repository": {
    "type": "git",
    "url": "https://github.com/bstaruk/starbase.git"
  },
  "bugs": {
    "url": "https://github.com/bstaruk/starbase/issues",
    "email": "brian@staruk.net"
  },
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "build": "npx webpack --config ./webpack/webpack.config.prod.js",
    "start": "npx webpack serve --config ./webpack/webpack.config.dev.js",
    "lint-staged": "lint-staged",
    "lint:js": "eslint src",
    "lint:css": "stylelint src",
    "prettify": "prettier --write",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.js": [
      "eslint --max-warnings=0"
    ],
    "*.scss": [
      "stylelint"
    ],
    "*.{js,jsx,json,md,scss}": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "babel-loader": "^8.2.3",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^6.6.0",
    "eslint": "^8.10.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-import-resolver-webpack": "^0.13.2",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-webpack-plugin": "^3.1.1",
    "html-loader": "^3.1.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.4",
    "mini-css-extract-plugin": "^2.5.3",
    "path": "^0.12.7",
    "prettier": "^2.5.1",
    "sass": "^1.49.9",
    "sass-loader": "^12.6.0",
    "style-loader": "^3.3.1",
    "stylelint": "^14.5.3",
    "stylelint-config-recommended": "^7.0.0",
    "stylelint-config-sass-guidelines": "^9.0.1",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-scss": "^4.1.0",
    "stylelint-webpack-plugin": "^3.1.1",
    "webpack": "^5.69.1",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.0",
    "webpack-merge": "^5.8.0"
  }
}
